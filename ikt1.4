import random
from random import randint

def insertion_sort(arr):
    for i in range(1, len(arr)):
        value = arr[i]
        j = i - 1
        while (j >= 0 and value < arr[j]):
            arr[j + 1] = arr[j]
            j = j - 1
        arr[j + 1] = value

