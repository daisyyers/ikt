import random

arr = random.sample(range(100), 20)
print(arr)

def insertion_sort(arr):
    for i in range(1, len(arr)):   
        j = i  
        value = arr[i]  
        while (j > 0 and value < arr[j-1]):   
            arr[j] = arr[j - 1]   
            j = j - 1  
        arr[j] = value
    return arr
insertion_sort(arr)

print('Sorted list: ', arr)
